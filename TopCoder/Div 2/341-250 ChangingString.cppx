typedef long long ll;
using namespace std;

class ChangingString {
public:
	int distance(string A, string B, int K) {
		vi diff(sz(A));
		rep(i,sz(A))
			diff[i] = abs(A[i] - B[i]);

		sort(rall(diff));

		int res =0;
		rep(i,sz(diff)){
			if(i<K){
				if(diff[i]==0)
					res++;
			}
			else
				res += diff[i];
		}
		return res;
	}
};

